# Copy this file to .env and fill values before deploying

# Google AI API Keys (from https://makersuite.google.com/app/apikey)
GEMINI_API_KEY_1=
GEMINI_API_KEY_2=
GEMINI_API_KEY_3=

# Authentication
AUTH_SECRET=replace_with_a_strong_secret

# Server
NODE_ENV=production
PORT=3000

# Supabase (Postgres + Storage)
SUPABASE_URL=https://clrhynfxwrirfbxhzikg.supabase.co
# Use a service role key for server-side operations (store securely in Render)
SUPABASE_SERVICE_KEY=your-supabase-service-key
# Or an anon key for limited operations
SUPABASE_ANON_KEY=

# Supabase storage bucket name used by the app
SUPABASE_FIR_BUCKET=fir-uploads

# Logging
LOG_LEVEL=info
ENABLE_DETAILED_LOGGING=false
