# Environment Variables Template
# Copy this to .env and fill in your values

# ==================== SUPABASE ====================
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_KEY=your_supabase_service_key
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_FIR_BUCKET=fir-uploads

# ==================== GOOGLE GEMINI AI ====================
GEMINI_API_KEY_1=your_primary_gemini_api_key
GEMINI_API_KEY_2=your_secondary_gemini_api_key
GEMINI_API_KEY_3=your_backup_gemini_api_key

# ==================== ML MODEL ====================
ML_MODEL_ENABLED=true
ML_MODEL_ENDPOINT=https://your-ml-model.onrender.com/predict

# ==================== AUTHENTICATION ====================
AUTH_SECRET=your_secure_random_secret_key_here

# ==================== SERVER ====================
NODE_ENV=production
PORT=3000
